REM *** THIS LOOKS LIKE A MESS, BUT IT IS BECAUSE I DO NOT HAVE RENUMBER ***
1 GOTO 100
2 N=ASC("0")
3 B$=CHR$($60):V$=CHR$($7D)
4 GOTO 6
5 MOD%=INT((X/Y-INT(X/Y))*Y+.5):RETURN
6 PRINT:PRINT:PRINT:PRINT "  ";V$;
7 FOR I=ASC("0") TO ASC("9"):PRINT CHR$(I);" ";:NEXT
8 FOR I=ASC("A") TO ASC("F"):PRINT CHR$(I);" ";:NEXT
9 PRINT:PRINT B$;B$;CHR$($7B);:FOR I=1 TO 31:PRINT B$;:NEXT
10 FOR I=$20 TO $7F
11 GOSUB 13
12 GOTO 30
13 X=I:Y=16:GOSUB 5
14 IF MOD%=0 AND I<>$20 THEN PRINT:PRINT "  ";V$;
15 IF MOD%=0 THEN PRINT:PRINT CHR$(I/16+N);"0";V$;
20 PRINT CHR$(I);" ";
25 RETURN
30 NEXT
35 N=ASC("7")
40 FOR I=$A0 TO $FF
50 GOSUB 13
60 NEXT
70 PRINT CHR$(19)
80 RETURN
90 D=1000:GOSUB 200
93 PRINT CHR$(19)
95 RETURN
100 IF PEEK($D9)<>40 THEN SYS$FF5F:PRINT
105 GOSUB 2
110 PRINT CHR$(14):PRINT "PETSCII (SHIFTED)  "
115 GOSUB 90
120 PRINT CHR$(142):PRINT "PETSCII (UNSHIFTED)"
130 GOSUB 90
140 GOTO 110
200 T1=TIME
210 D=60*D/1000
220 IF TIME-T1<D THEN GOTO 220
230 RETURN
RUN
  
